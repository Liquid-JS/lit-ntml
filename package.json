{
  "name": "lit-ntml",
  "version": "0.3.6",
  "description": "Inspired by lit-html but for Node.js",
  "keywords": [
    "html",
    "lit-ntml",
    "minify",
    "npm",
    "ntml",
    "parse-html",
    "prettify",
    "tagged-template-literals",
    "template",
    "templating"
  ],
  "homepage": "https://github.com/motss/lit-ntml",
  "license": "MIT",
  "author": "Rong Sen Ng <wes.ngrongsen@gmail.com> (https://github.com/motss)",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/lit-ntml.git"
  },
  "scripts": {
    "build": "npm run ts && cross-env NODE_ENV=production gulp build && npm run cleanup",
    "build:debug": "npm run ts && gulp build && npm run cleanup",
    "check": "npm run lint && npm run nsp",
    "cleanup": "rm -rf gulpfile.js",
    "debug": "node --inspect --expose-gc --max-old-space-size=8192 -r @std/esm dist/demo",
    "demo": "node -r @std/esm demo/",
    "gulp": "npm run ts && gulp",
    "lint": "npm run ts && cross-env NODE_ENV=production gulp lint && npm run cleanup",
    "nsp": "nsp check",
    "prepublishOnly": "npm run check && npm run build",
    "test": "ava ./test",
    "ts": "tsc --target esnext --lib esnext,esnext.asynciterable --module commonjs gulpfile.ts"
  },
  "dependencies": {
    "pretty": "^2.0.0"
  },
  "devDependencies": {
    "@std/esm": "^0.20.0",
    "@types/express": "^4.11.1",
    "@types/html-minifier": "^3.5.1",
    "@types/node": "^9.4.2",
    "@types/parse5": "^3.0.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.3.0",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-sequence": "^1.0.0",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^4.0.0",
    "nsp": "^3.1.0",
    "quick-lru": "^1.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "tslint-consistent-codestyle": "^1.11.1",
    "tslint-immutable": "^4.5.1",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "@std/esm": "cjs",
  "ava": {
    "require": [
      "@std/esm"
    ]
  }
}
